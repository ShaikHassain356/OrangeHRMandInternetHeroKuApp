<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="tests">

    <listeners>
        <listener class-name="Utilities.ExtentReportManager"/>
        <listener class-name="org.testng.reporters.FailedReporter"/>
    </listeners>
    <test verbose="2" preserve-order="true" name="firefox-test">

        <parameter name="os" value="windows"/>
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="TestCases.TC001_addorremoveelement"/>
            <class name="TestCases.TC002_BasicAuth"/>
            <class name="TestCases.TC003_contextmenu"/>
            <class name="TestCases.TC004_draganddrop"/>
        </classes>
    </test>

</suite>